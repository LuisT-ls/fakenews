{
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/offline",
      "dest": "/pages/offline.html"
    },
    {
      "src": "/(.*)",
      "dest": "/404.html",
      "status": 404
    },
    {
      "src": "/api/getApiKey",
      "methods": ["GET"],
      "headers": {
        "Access-Control-Allow-Origin": "https://fakenews-sigma.vercel.app"
      }
    }
  ]
}
